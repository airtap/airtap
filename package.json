{
  "name": "airtap",
  "version": "2.0.2",
  "description": "JavaScript browser testing in the cloud",
  "main": "lib/airtap.js",
  "dependencies": {
    "batch": "~0.6.1",
    "body-parser": "~1.18.3",
    "browserify": "~16.2.3",
    "browserify-istanbul": "~3.0.1",
    "chalk": "^2.3.1",
    "commander": "~2.19.0",
    "compression": "~1.7.1",
    "convert-source-map": "~1.6.0",
    "debug": "~4.1.0",
    "express": "~4.16.2",
    "express-state": "~1.4.0",
    "find-nearest-file": "~1.1.0",
    "firefox-profile": "~1.2.0",
    "globs-to-files": "~1.0.0",
    "hbs": "~4.0.1",
    "highlight.js": "~9.12.0",
    "http-proxy": "~1.17.0",
    "humanize-duration": "~3.18.0",
    "load-script": "~1.0.0",
    "lodash": "~4.17.5",
    "mkdirp": "~0.5.1",
    "opener": "~1.5.0",
    "sauce-browsers": "~2.0.0",
    "server-destroy": "~1.0.1",
    "shell-quote": "~1.6.1",
    "split2": "^3.0.0",
    "stack-mapper": "~0.2.2",
    "stacktrace-js": "~2.0.0",
    "tap-finished": "~0.0.1",
    "tap-parser": "~5.4.0",
    "watchify": "~3.11.0",
    "wd": "~1.11.1",
    "xhr": "~2.5.0",
    "yamljs": "~0.3.0"
  },
  "devDependencies": {
    "airtap-browsers": "^0.2.0",
    "cross-env": "~5.2.0",
    "dependency-check": "^3.0.0",
    "electron": "^2.0.3",
    "hallmark": "~0.1.0",
    "standard": "^12.0.0",
    "tape": "^4.8.0",
    "through2": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/airtap/airtap.git"
  },
  "bin": {
    "airtap": "./bin/airtap.js"
  },
  "keywords": [
    "airtap",
    "browser",
    "saucelabs",
    "tap",
    "tape",
    "testing"
  ],
  "author": "Roman Shtylman <shtylman@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "standard && hallmark && npm run dependency-check && cross-env DEBUG=airtap* tape test",
    "dependency-check": "dependency-check package.json --missing --unused -i dependency-check -i cross-env -i standard -i hallmark --entry test/index.js --entry test/fixtures/*/*.js --entry client/*.js",
    "hallmark": "hallmark --fix"
  },
  "engines": {
    "node": ">=6"
  }
}
