{
  "name": "airtap",
  "version": "3.0.0",
  "description": "JavaScript browser testing in the cloud",
  "main": "lib/airtap.js",
  "dependencies": {
    "aggregate-error": "~3.0.1",
    "browserify": "~16.5.0",
    "browserify-istanbul": "~3.0.1",
    "chalk": "^3.0.0",
    "commander": "~4.0.0",
    "compression": "~1.7.1",
    "debug": "~4.1.0",
    "engine.io": "~3.4.0",
    "engine.io-client": "~3.3.2",
    "express": "~4.17.0",
    "find-nearest-file": "~1.1.0",
    "firefox-profile": "~1.2.0",
    "globs-to-files": "~1.0.0",
    "http-proxy": "~1.18.0",
    "humanize-duration": "~3.21.0",
    "load-script": "~1.0.0",
    "lodash": "~4.17.5",
    "mkdirp": "~0.5.1",
    "ms": "~2.1.2",
    "opener": "~1.5.0",
    "run-parallel-limit": "^1.0.5",
    "sauce-browsers": "~2.0.0",
    "sauce-connect-launcher": "~1.3.1",
    "server-destroy": "~1.0.1",
    "shell-quote": "~1.7.0",
    "tap-parser": "~10.0.1",
    "uuid": "^3.4.0",
    "watchify": "~3.11.0",
    "wd": "~1.12.0",
    "yamljs": "~0.3.0"
  },
  "devDependencies": {
    "airtap-browsers": "^0.2.0",
    "cross-env": "^6.0.0",
    "dependency-check": "^4.0.0",
    "electron": "^7.1.8",
    "hallmark": "^2.0.0",
    "standard": "^14.0.0",
    "tape": "^4.8.0",
    "through2": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/airtap/airtap.git"
  },
  "bin": {
    "airtap": "./bin/airtap.js"
  },
  "keywords": [
    "airtap",
    "browser",
    "saucelabs",
    "tap",
    "tape",
    "testing"
  ],
  "author": "Roman Shtylman <shtylman@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "standard && hallmark && npm run dependency-check && cross-env DEBUG=airtap* tape test",
    "dependency-check": "dependency-check package.json --missing --unused -i dependency-check -i cross-env -i standard -i hallmark test/index.js test/fixtures/*/*.js client/*.js",
    "hallmark": "hallmark --fix"
  },
  "engines": {
    "node": ">=10"
  },
  "greenkeeper": {
    "ignore": [
      "engine.io-client"
    ]
  }
}
