{
  "name": "airtap",
  "version": "0.0.1",
  "description": "JavaScript browser testing in the cloud",
  "main": "lib/zuul.js",
  "dependencies": {
    "JSON2": "~0.1.0",
    "batch": "~0.6.1",
    "browserify": "~13.3.0",
    "browserify-istanbul": "~3.0.1",
    "char-split": "~0.2.0",
    "colors": "~1.1.2",
    "commander": "~2.14.1",
    "compression": "~1.7.1",
    "convert-source-map": "~1.5.1",
    "debug": "~3.1.0",
    "express": "~4.16.2",
    "express-state": "~1.4.0",
    "find-nearest-file": "~1.1.0",
    "firefox-profile": "~1.1.0",
    "globs-to-files": "~1.0.0",
    "hbs": "~4.0.1",
    "highlight.js": "~9.12.0",
    "http-proxy": "~1.16.2",
    "humanize-duration": "~3.12.1",
    "istanbul-middleware": "~0.2.2",
    "load-script": "~1.0.0",
    "lodash": "~4.17.5",
    "opener": "~1.4.3",
    "osenv": "~0.1.5",
    "shallow-copy": "~0.0.1",
    "shell-quote": "~1.6.1",
    "stack-mapper": "~0.2.2",
    "stacktrace-js": "~2.0.0",
    "superagent": "~3.8.2",
    "tap-finished": "~0.0.1",
    "tap-parser": "0.7.0",
    "watchify": "~3.10.0",
    "wd": "~1.5.0",
    "xtend": "~4.0.1",
    "yamljs": "~0.3.0"
  },
  "devDependencies": {
    "after": "~0.8.1",
    "airtap-browsers": "0.0.1",
    "bulk-require": "^1.0.1",
    "cross-env": "~5.1.3",
    "dependency-check": "^2.10.0",
    "electron-prebuilt": "^1.4.13",
    "mocha": "~1.16.2",
    "phantomjs-prebuilt": "^2.1.16",
    "tape": "^4.8.0",
    "through2": "^2.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/airtap/airtap.git"
  },
  "bin": {
    "airtap": "./bin/airtap"
  },
  "keywords": [
    "airtap",
    "testing",
    "browser",
    "qunit",
    "mocha",
    "saucelabs"
  ],
  "author": "Roman Shtylman <shtylman@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "npm run dependency-check && cross-env DEBUG=airtap* mocha --ui qunit --timeout 0 --bail -- test/index.js",
    "dependency-check": "dependency-check package.json --missing --unused -i dependency-check -i mocha -i phantomjs -i electron -i cross-env --entry test/index.js --entry test/integration/*.js --entry test/fixtures/*/*.js --entry test/unit/*.js --entry frameworks/zuul.js --entry frameworks/tape/*.js"
  },
  "engines": {
    "node": ">=4"
  }
}
